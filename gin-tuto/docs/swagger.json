    "definitions": {
        "model.StGrafanaAlert": {
            "type": "object",
            "properties": {
                "dashboardId": {
                    "type": "integer"
                },
                "evalMatches": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.stEvalMatch"
                    }
                },
                "imageUrl": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                },
                "orgId": {
                    "type": "integer"
                },
                "panelId": {
                    "type": "integer"
                },
                "ruleId": {
                    "type": "integer"
                },
                "ruleName": {
                    "type": "string"
                },
                "ruleUrl": {
                    "type": "string"
                },
                "state": {
                    "type": "string"
                },
                "tags": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "title": {
                    "type": "string"
                }
            }
        },
        "model.StUniversalProducerEMail": {
            "type": "object",
            "required": [
                "content",
                "from",
                "producer",
                "title",
                "to"
            ],
            "properties": {
                "cc": {
                    "type": "string",
                    "example": "xxx@yyyy.com;yyy@xxxx.co.kr"
                },
                "content": {
                    "type": "string"
                },
                "from": {
                    "type": "string"
                },
                "producer": {
                    "type": "string",
                    "example": "wiss or wingo or kiss or more"
                },
                "tags": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    },
                    "example": {
                        " region": "KR",
                        "game": "#MUA2"
                    }
                },
                "title": {
                    "type": "string"
                },
                "to": {
                    "type": "string",
                    "example": "xxx@yyyy.com;yyy@xxxx.co.kr"
                }
            }
        },
        "model.StUniversalProducerTeams": {
            "type": "object",
            "required": [
                "content",
                "from",
                "producer",
                "title",
                "touri"
            ],
            "properties": {
                "content": {
                    "type": "string"
                },
                "from": {
                    "type": "string"
                },
                "producer": {
                    "type": "string",
                    "example": "wiss or wingo or kiss or more"
                },
                "tags": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    },
                    "example": {
                        " region": "KR",
                        "game": "#MUA2"
                    }
                },
                "title": {
                    "type": "string"
                },
                "touri": {
                    "type": "string",
                    "example": "http://xxx.x.xx.xxx.x."
                }
            }
        },
        "model.stEvalMatch": {
            "type": "object",
            "properties": {
                "metric": {
                    "type": "string"
                },
                "tags": {
                    "$ref": "#/definitions/model.stEvalMatchTags"
                },
                "value": {
                    "type": "integer"
                }
            }
        },
        "model.stEvalMatchTags": {
            "type": "object"
        }
    }
